{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "env": {
        "API_URL": "https://expense-trackerr-server.vercel.app"
    },
    "crons": [
        {
        "path": "/api/cron",
        "schedule": "50 14 * * *"
        }
    ]
}